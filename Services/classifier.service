[Unit]
Description=Usecase Classifier
SourcePath=/usr/bin/classifier
After=multi-user.target

[Service]
User=root
Group=root
Restart=always
RemainAfterExit=yes
ExecStart=/usr/bin/classifier
TimeoutStopSec=1

[Install]
WantedBy=multi-user.target
